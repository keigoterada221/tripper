<% if user.status.present? %>
	<%= button_to "有効会員",admin_user_path(user.id),method: :patch, remote: true, params: {user_status: "有効会員"},class: "btn btn-primary mb-2 mr-3" %>
<% else %>
	<%= button_to "退会済み",admin_user_path(user.id),method: :patch, remote: true, params: {user_status: "退会済み"},class: "btn btn-danger mb-2 mr-3" %>
<% end %>